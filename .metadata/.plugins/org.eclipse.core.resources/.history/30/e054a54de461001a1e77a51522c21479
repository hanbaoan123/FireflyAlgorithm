package domain;

public class ObjectiveFun {
	/**
	 * 自变量取值范围
	 */
	private Range range;

	public ObjectiveFun() {
		super();
		double[] st = new double[2];
		double[] en = new double[2];
		st[0] = st[1] = -100;
		en[0] = en[1] = 100;
		this.range=new Range(2, [-100,100], [-100,100]);
		
	}

	public ObjectiveFun(Range range) {
		super();
		this.range = range;
	}

	/**
	 * 返回目标函数值
	 * 
	 * @param positionCode
	 * @return
	 */
	public double getObjValue(double[] positionCode) {
		double son = Math.sin(Math.sqrt((positionCode[0] * positionCode[0] + positionCode[1] * positionCode[1])));
		son = son * son - 0.5;
		double mot = 1 + 0.001 * (positionCode[0] * positionCode[0] + positionCode[1] * positionCode[1]);
		mot = mot * mot;
		return son / mot - 0.5;
	}

	public Range getRange() {
		return range;
	}

	public void setRange(Range range) {
		this.range = range;
	}
}
